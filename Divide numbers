public class Main {
  public static void main (String[] args) {
  Scanner scanner = new Scanner (System.in);
  System.out.print("Number: ");
  int number = scanner.nextInt();    // display "Number: " and then input the number
  
  if(number % 5 == 0 && number % 3 == 0)  // if the number is divided by 5 and 3 then display FizzBuzz 
    System.out.println("FizzBuzz");
  else if(number % 5 == 0)   // if the number is divide by 5 then display Fizz
    System.out.println("Fizz");
  else if(number % 3 == 0)   // if the number is divide by 3 then display Buzz
    System.out.println("Buzz");
  else System.out.println(number);
 }
}
